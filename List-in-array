import React, { Component } from 'react'

class List extends Component {

FruitsListMap=(a)=>{
    return  <option>{a}</option>
}
    render() {
        const FruitList=['Banana','Green Apple','Mango','Guava','Orange'];
        const FruitListMap=FruitList.map(this.FruitsListMap);
        return (
                <div className="text-center">
                     <select name="" id="">{FruitListMap}</select>
                </div>
               
        )
    }
}

export default List
